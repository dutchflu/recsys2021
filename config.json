{
  "pipeline_type": "training_evaluation_pipeline",
  "data_provider_config": {
    "load_config": {
      "raw_data_path": "../data/part-00000.csv",
      "split_size": 0.2
    },
    "data_config": {
      "label_cols": ["reply_timestamp", "retweet_timestamp", "retweet_with_comment_timestamp", "like_timestamp"],
      "custom_features": ["engaged_with_user_id", "enaging_user_id", "language", "tweet_type"],
      "categorical_features": "auto"
    }
  },
  "model_config": {
    "model_type": "light_gbm",
    "hyperparameters": {
      "objective": "multiclass",
      "boosting_type": "gbdt",
      "importance_type": "gain",
      "num_leaves": 5,
      "max_depth": 6,
      "n_estimators": 10,
      "learning_rate": 1e-1,
      "min_split_gain": 0,
      "min_child_weight": 1e-3,
      "min_child_samples": 20,
      "subsample": 1.0,
      "subsample_freq": 10,
      "colsample_bytree": 1.0,
      "n_jobs": -1
    }
  },
  "train_config":{
    "val_size": 0.20
  }
}
